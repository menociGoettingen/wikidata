{
    "name": "menoci/wikidata",
    "description": "A PHP client for working with Wikidata API.",
    "homepage": "https://github.com/menociGoettingen/wikidata",
    "license": "MIT",
    "authors": [
        {
            "name": "Tammo Klaassen"
        }
    ],
    "require": {
        "tightenco/collect": "^8.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Wikidata\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Wikidata\\Tests\\": "tests"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    }
}
